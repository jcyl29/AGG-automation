{
  "name": "AyakashiChrome",
  "version": "1.0",
  "manifest_version": 2,
  "description": "AyakashiChrome",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "http://zc2.ayakashi.zynga.com/*",
    "\u003Call_urls\u003E",    
    "webRequest",
    "webRequestBlocking",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
      "scripts": [ "background.js" ]
   }
}